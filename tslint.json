{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "object-literal-sort-keys": false,
        "ordered-imports": false,
        "semicolon": true,
        "quotemark": false,
        "arrow-parens": false,
        "one-variable-per-declaration": false,
        "object-literal-shorthand": false,
        "no-trailing-whitespace": false,
        "curly": false,
        "triple-equals": false,
        "no-consecutive-blank-lines": false,
        "no-bitwise": false
    },
    "rulesDirectory": []
}